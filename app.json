{
  "expo": {
    "name": "Taiga Open",
    "slug": "taiga-open-app",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.modernmasters.taiga-open"
    },
    "android": {
      "package": "org.modernmasters.taiga-open"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}